<!DOCTYPE html>
<html>
<head>
<title> <%= @title %> | Survey Engine </title>
<%= csrf_meta_tag %>
<%= render 'layouts/stylesheets' %>

</head>

<body>
<div class="container">

<br></br><br></br>
<%= render 'layouts/header' %>
<br></br><br></br><br></br>

<section class="round">
<% flash.each do |key, value| %>
<div class="flash <%= key %>"><%= value %></div>
<% end %>

<%= yield %>
</section>
<%=  render 'layouts/footer'%>
<%= debug(params) if Rails.env.development? %>

</div>
</body>


</html>
